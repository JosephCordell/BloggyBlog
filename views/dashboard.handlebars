<h1> Welcome </h1>

<div class="posts">
  {{#each posts}}
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
        <a href="/edit/{{id}}">{{title}}</a>
      </h2>
    </div>
    <div class="card-body">
      {{body}}
    </div>
  </article>
  {{/each}}
</div>


  <footer>
    <a href="/dashboard/new" class="btn btn-primary" > New Post</a>
  </footer>